{
  "kind": "collectionType",
  "collectionName": "contacts",
  "info": {
    "singularName": "contact",
    "pluralName": "contacts",
    "displayName": "Contact",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "phone": {
      "type": "string"
    },
    "eventType": {
      "type": "enumeration",
      "enum": [
        "wedding",
        "corporate",
        "private",
        "festival",
        "other"
      ]
    },
    "eventDate": {
      "type": "date"
    },
    "message": {
      "type": "text",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "new",
        "contacted",
        "quoted",
        "booked",
        "completed"
      ],
      "default": "new"
    }
  }
}